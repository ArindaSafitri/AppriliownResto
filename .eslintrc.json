{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
    "no-prototype-builtins": "off",
    "consistent-return": "off",
    "no-underscore-dangle": 0,
    "linebreak-style": 0,
    "no-restricted-globals": 0,
    "no-unused-vars": 0,
    "import/no-extraneous-dependencies ": "off",
    "import-order": "off",
    "no-console": 0,
    "no-undef": 0,
    "prefer-destructuring": 0
    }
}
